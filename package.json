{
    "name": "ms_mcp",
    "version": "1.0.0",
    "description": "Microservice mcp.",
    "author": "cimo",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cimo/Ms_mcp.git"
    },
    "type": "module",
    "scripts": {
        "preinstall": "npx -y npm-force-resolutions",
        "remove_dist": "node -e \"const fs = require('fs'); try{ fs.rmdirSync('./dist/', { recursive: true }) } catch{}; process.exit(0);\"",
        "check": "eslint --no-cache --config eslint.config.js",
        "build_tsc": "npm run remove_dist && tsc --build tsconfig.tsc.json",
        "build": "npm run build_tsc && npm run check",
        "execute": "nodemon --config nodemon.json"
    },
    "dependencies": {
        "@cimo/authentication": "1.1.31",
        "@cimo/cronjob": "1.0.10",
        "@cimo/environment": "1.0.23",
        "@cimo/form-data_parser": "1.1.39",
        "@cimo/request": "1.6.37",
        "@modelcontextprotocol/sdk": "1.26.0",
        "@nut-tree-fork/nut-js": "4.2.6",
        "chrome-launcher": "1.2.1",
        "cookie-parser": "1.4.7",
        "cors": "2.8.5",
        "express": "4.22.1",
        "express-rate-limit": "7.5.0",
        "officeparser": "6.0.4",
        "sharp": "0.34.5",
        "zod": "4.3.6"
    },
    "devDependencies": {
        "@types/cookie-parser": "1.4.8",
        "@types/cors": "2.8.17",
        "@types/express": "5.0.0",
        "@types/node": "22.10.2",
        "@typescript-eslint/eslint-plugin": "8.24.1",
        "@typescript-eslint/parser": "8.24.1",
        "eslint": "9.31.0",
        "eslint-plugin-prettier": "5.2.1",
        "nodemon": "3.1.9",
        "prettier": "3.4.2",
        "typescript": "5.7.2"
    },
    "resolutions": {
        "node-fetch": "2.6.7",
        "**/node-fetch": "2.6.7",
        "crypto-js": "4.2.0",
        "**/crypto-js": "4.2.0",
        "path-to-regexp": "0.1.12",
        "**/path-to-regexp": "0.1.12",
        "send": "0.19.0",
        "**/send": "0.19.0",
        "serve-static": "1.16.0",
        "**/serve-static": "1.16.0",
        "body-parser": "1.20.3",
        "**/body-parser": "1.20.3",
        "cookie": "0.7.0",
        "**/cookie": "0.7.0",
        "brace-expansion": "2.0.2",
        "**/brace-expansion": "2.0.2"
    }
}
